<!DOCTYPE html>
<html>
<head>
  <title>Add Car</title>
  <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="addCar.css">
  <script type="text/javascript" src="addCar.js"></script>
</head>
<body>


<div class="container">
  <div class="row">
    <form class="form-horizontal">
            <fieldset>
                <!-- Form Name -->
                <legend>Fill in Vehicle Details</legend>


                <!-- Select Basic -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="comparison">Type of Vehicle</label>
                    <div class="col-md-4">
                        <select id="comparison" name="comparison" class="form-control">
                            <option value="car">4-wheel car</option>
                            <option value="bicycle">bicycle</option>
                            <option value="motorbike">motorbike</option>
                        </select>
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="textinput">License Plate ID</label>
                    <div class="col-md-6">
                        <input id="textinput" name="textinput" type="text" placeholder="DQ1020" class="form-control input-md" required="">
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="textinput">Full Name of Car Owner</label>
                    <div class="col-md-6">
                        <input id="textinput" name="textinput" type="text" placeholder="Should be the name of legal car owner as stated on the car registration document" class="form-control input-md" required="">
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="color">Brand</label>
                    <div class="col-md-6">
                        <input id="color" name="color" type="text" placeholder="Audi" class="form-control input-md">
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="color">Model</label>
                    <div class="col-md-6">
                        <input id="color" name="color" type="text" placeholder="R8" class="form-control input-md">
                    </div>
                </div>

                <!-- Select Basic -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="comparison">Year</label>
                    <div class="col-md-4">
                        <select id="comparison" name="comparison" class="form-control">
                            <option value="2018">2018</option>
                            <option value="2017">2017</option>
                            <option value="2016">2016</option>
                            <option value="2015">2015</option>
                            <option value="2014">2014</option>
                            <option value="2013">2013</option>
                            <option value="2012">2012</option>
                            <option value="2011">2011</option>
                            <option value="2010">2010</option>
                            <option value="2009">2009</option>
                            <option value="2008">2008</option>
                            <option value="2007">2007</option>
                            <option value="2006">2006</option>
                            <option value="2005">2005</option>
                            <option value="2004">2004</option>
                            <option value="2003">2003</option>
                            <option value="2002">2002</option>
                            <option value="2001">2001</option>
                            <option value="2000">2000</option>
                            <option value="1999">1999</option>
                            <option value="1998">1998</option>
                            <option value="1997">1996</option>
                            <option value="1996">1995</option>
                            <option value="1995">1994</option>
                            <option value="1994">1993</option>
                            <option value="1993">1992</option>
                            <option value="1992">1991</option>
                            <option value="1991">1990</option>
                            <option value="1990">Before 1990</option>
                        </select>
                    </div>
                </div>


                <!-- Multiple Checkboxes (inline) -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="notification">Available Space</label>

                    <div class="col-md-4">
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Front Door - Driver Side
                        </label>
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Hood
                        </label>
                    </div>

                    <div class="col-md-4">
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Back Door - Driver Side
                        </label>
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Fender - Driver Side
                        </label>
                    </div>

                    <div class="col-md-4">
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Front Door - Passenger Side
                        </label>
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Fender - Passenger Side
                        </label>
                    </div>

                    <div class="col-md-4">
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Back Door - Driver Side
                        </label>
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Fender - Driver Side
                        </label>
                    </div>

                    <div class="col-md-4">
                        <label class="checkbox-inline" for="notification-0">
                            <input type="checkbox" name="notification" id="notification-0" value="1"> Back Door - Passenger Side
                        </label>
                    </div>

                </div>

                <!-- Multiple Radios (inline) -->
<!--                 <div class="form-group">
                    <label class="col-md-4 control-label" for="reqType">Request Type</label>
                    <div class="col-md-4">
                        <label class="radio-inline" for="reqType-0">
                            <input type="radio" name="reqType" id="reqType-0" value="post" checked="checked"> POST
                        </label>
                        <label class="radio-inline" for="reqType-1">
                            <input type="radio" name="reqType" id="reqType-1" value="get"> GET
                        </label>
                    </div>
                </div> -->

                  <form class="container" id="form1" enctype="multipart/form-data" method="post" action="Upload.aspx">
                    <div class="row setup-content" id="step-1">
                        <div class="col-xs-12">
                            <div class="col-md-12 well text-center">

                      <label for="fileToUpload">Select a File to Upload</label><br />
                      <input type="file" name="fileToUpload" id="fileToUpload" onchange="fileSelected();"/>
                    </div>
                    <div id="fileName"></div>
                    <div id="fileSize"></div>
                    <div id="fileType"></div>
                    <div class="row">
                      <input type="button" onclick="uploadFile()" value="Upload" />
                    </div>
                    <div id="progressNumber"></div>

                            </div>
                        </div>
                    </div>
                  </form>

                <!-- Button (Double) -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="submitButton"></label>
                    <div class="col-md-8">
                        <button id="submitButton" name="submitButton" class="btn btn-success">Save</button>
                        <button id="cancel" name="cancel" class="btn btn-inverse">Cancel</button>
                    </div>
                </div>
            </fieldset>









        </form>
  </div>
</div>


<script src="https://code.jquery.com/jquery-2.1.4.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
</body>
</html>
